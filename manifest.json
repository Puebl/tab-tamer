{
  "manifest_version": 3,
  "name": "Tab Tamer",
  "version": "0.1.0",
  "description": "Сохраняйте и восстанавливайте сессии вкладок.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Tamer"
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  }
}
